{
  "name": "fanyi",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "contextMenus",
    "notifications",
    "webRequest"
  ],
  "action": {
    "default_title": "My Bookmarks",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["content_load.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
